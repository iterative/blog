{
  "name": "dvc-blog",
  "private": true,
  "description": "Blog for dvc.org",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "engines": {
    "node": "^12.0.0"
  },
  "scripts": {
    "build": "gatsby build",
    "heroku-postbuild": "./scripts/deploy-with-s3.js",
    "develop": "gatsby develop",
    "start": "./scripts/clear-cloudflare-cache.js; node ./src/server/server.js",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "lint-ts": "tslint --project .",
    "lint-css": "stylelint \"src/**/*.css\"",
    "format-all": "prettier --write './**/*.{js,jsx,md,tsx,ts,json}'",
    "check-all": "prettier --check './**/*.{js,jsx,md,tsx,ts,json}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn typecheck && yarn test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn prettier --write",
      "yarn lint-ts",
      "git add"
    ],
    "*.md": [
      "yarn prettier --write",
      "git add"
    ],
    "*.css": [
      "yarn stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@hapi/wreck": "^17.0.0",
    "@reach/portal": "^0.5.0",
    "@reach/tooltip": "^0.5.4",
    "@types/google.analytics": "^0.0.40",
    "classnames": "^2.2.6",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "gatsby": "^2.19.19",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-twitter": "^2.1.18",
    "gatsby-remark-embed-gist": "^1.1.9",
    "gatsby-remark-embedder": "^1.8.0",
    "gatsby-remark-relative-images": "^0.2.3",
    "hast-util-select": "^3.0.1",
    "mime-types": "^2.1.26",
    "node-fetch": "^2.6.0",
    "prismjs": "^1.17.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-use": "^13.6.3",
    "rehype-parse": "^6.0.2",
    "rehype-stringify": "^6.0.1",
    "serve-handler": "^6.1.2",
    "unist-util-visit": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@svgr/webpack": "^4.3.3",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "autoprefixer": "^9.7.1",
    "fs-extra": "^8.1.0",
    "gatsby-plugin-feed": "^2.3.26",
    "gatsby-plugin-google-analytics": "^2.1.33",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-postcss": "^2.1.19",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sharp": "2.3.13",
    "gatsby-plugin-sitemap": "^2.2.26",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.1.26",
    "gatsby-remark-copy-linked-files": "^2.1.36",
    "gatsby-remark-images": "^3.1.42",
    "gatsby-remark-prismjs": "^3.3.30",
    "gatsby-remark-responsive-iframe": "^2.2.31",
    "gatsby-remark-smartypants": "^2.1.20",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^2.6.48",
    "gatsby-transformer-sharp": "^2.3.13",
    "graphql": "^14.5.8",
    "husky": "^3.0.9",
    "jest-cli": "^25.1.0",
    "lint-staged": "^9.4.2",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.0.2",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.1",
    "prettier": "^1.19.1",
    "remark-html": "^10.0.0",
    "s3-client": "^4.4.2",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.2"
  },
  "cacheDirectories": [
    "node_modules",
    ".cache"
  ]
}
